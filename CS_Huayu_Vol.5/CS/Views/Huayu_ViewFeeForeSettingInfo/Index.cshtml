@model Model.M_WayBillWeight
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Index</title>
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/jquery.easyui.min.js")" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/black/easyui.css")" rel="stylesheet"
        title="black" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/bootstrap/easyui.css")" rel="stylesheet"
        title="bootstrap"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/cupertino/easyui.css")" rel="stylesheet"
        title="cupertino"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/dark-hive/easyui.css")" rel="stylesheet"
        title="dark-hive"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/default/easyui.css")" rel="stylesheet"
        title="default"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/gray/easyui.css")" rel="stylesheet"
        title="gray"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro/easyui.css")" rel="stylesheet"
        title="metro"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-blue/easyui.css")" rel="stylesheet"
        title="metro-blue"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-gray/easyui.css")" rel="stylesheet"
        title="metro-gray"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-green/easyui.css")" rel="stylesheet"
        title="metro-green"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-orange/easyui.css")" rel="stylesheet"
        title="metro-orange"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-red/easyui.css")" rel="stylesheet"
        title="metro-red"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/pepper-grinder/easyui.css")" rel="stylesheet"
        title="pepper-grinder"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/sunny/easyui.css")" rel="stylesheet"
        title="sunny"/>
    <link href="@Url.Content("~/JqueryUI/themes/icon.css")"  rel="stylesheet" type="text/css" />
    <script  src="@Url.Content("~/JS/Huayu/Huayu_ViewFeeForeSettingInfo.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/Common/Common.js")" type="text/javascript"></script>
</head>
<body>
    <div id="p" class="easyui-panel" style="width: 990px; height: 110px; padding: 1px;
        background: #fafafa;" data-options="iconCls:'icon-save',closable:false,    
                collapsible:false,minimizable:false,maximizable:false">
        <table>
            <tr>
                <td colspan="15">
                    <table>
                        <tr>
                            <td>
                                总运单号:
                            </td>
                            <td>
                                <input  type="hidden" id="hid_wbId" name="hid_wbId" value='@ViewData["WBID"]'/>
                                <input  type="text" id="WBSerialNum" name="WBSerialNum" value='@ViewData["WBSerialNum"]' readonly  style="width: 120px"/>
                            </td>
                            <td>
                                货代公司:
                            </td>
                            <td>
                                <input  type="text" id="txtWBCompany" name="txtWBCompany" value='@ViewData["WBCompany"]' readonly  style="width: 100px"/>
                            </td>
                            <td>
                                起运地/目的地:
                            </td>
                            <td>
                                <input  type="text" id="txtSRPort" name="txtSRPort" value='@ViewData["WBSRport"]' readonly style="width: 100px"/>
                            </td>
                            <td>
                                计费重量:
                            </td>
                            <td>
                                <input  type="text" id="txtActualWeight" name="txtActualWeight" value='@Model.ActualWeight' readonly style="width: 60px"/>
                            </td>
                            <td>
                                到货日期:
                            </td>
                            <td>
                                <input  type="text" id="txtArrivalDate" name="txtArrivalDate" value='@ViewData["WBArrivalDate"]' readonly style="width: 80px"/>
                            </td>
                            <td>
                                报关类别:
                            </td>
                            <td>
                                <input  type="hidden" id="hid_txtCustomCategory" name="hid_txtCustomCategory" value='@ViewData["txtCustomCategory"]'/>
                                <input  type="text" id="txtCustomCategory" name="txtCustomCategory" value='@ViewData["txtCustomCategory_Des"]' readonly  style="width: 70px"/>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td colspan="15">
                    <hr />
                </td>
            </tr>
            <tr>
                <td align="right">
                    直航\转关:
                    <input type="hidden" id="hid_wbwID" name="hid_wbwID"  value="@Model.wbwId"/>
                </td>
                <td>
                    <input type="text" id="ddlReceiptMode_ForSetting" name="ddlReceiptMode_ForSetting"
                        style="width: 60px" />
                    <input type="hidden" id="hid_ddlReceiptMode_ForSetting" name="hid_ddlReceiptMode_ForSetting"  value="@Model.ddlReceiptMode_ForSetting"/>
                </td>
                <td align="right">
                    操作费:
                </td>
                <td>
                    <input  type="text" style="width:60px" id="OperateFee_ForSetting" name="OperateFee_ForSetting" value='@Model.OperateFee_ForSetting' />
                </td>
                <td align="right">
                    提货费:
                </td>
                <td>
                    <input  type="text"  style="width:60px" id="PickGoodsFee_ForSetting" name="PickGoodsFee_ForSetting" value='@Model.PickGoodsFee_ForSetting' />
                </td>
                <td align="right">
                    移库费:
                </td>
                <td>
                    <input type="text" style="width:60px"  id="ShiftGoodsFee_ForSetting" name="ShiftGoodsFee_ForSetting"
                          value='@Model.ShiftGoodsFee_ForSetting'/>
                </td>
                <td align="right">
                    代收仓储费:
                </td>
                <td>
                    <input type="text" style="width: 60px" id="CollectionFee_ForSetting" name="CollectionFee_ForSetting"
                    value='@Model.CollectionFee_ForSetting' />
                </td>
                <td align="right">
                    系统申报费:
                </td>
                <td>
                    <input type="text" style="width: 60px" id="ReportSystem_ForSetting" name="ReportSystem_ForSetting"
                    value='@Model.ReportSystem_ForSetting' />
                </td>
                <td align="right">
                    检验检疫查验费:
                </td>
                <td>
                    <input type="text" style="width: 60px" id="QuarantineCheckFee_ForSetting" name="QuarantineCheckFee_ForSetting"
                    value='@Model.QuarantineCheckFee_ForSetting' />
                </td>
            </tr>
            <tr>
                <td align="right">
                    结算方式:
                </td>
                <td>
                    <input type="text" id="ddlPayMode_ForSetting" name="ddlPayMode_ForSetting" style="width: 60px" />
                    <input type="hidden" id="hid_ddlPayMode_ForSetting" name="hid_ddlPayMode_ForSetting"  value="@Model.ddlPayMode_ForSetting"/>
                </td>
                <td align="right">
                    应付单价:
                </td>
                <td>
                    <input type="text"  style="width:60px"  id="ShouldPayUnit_ForSetting" name="ShouldPayUnit_ForSetting"
                          value='@Model.ShouldPayUnit_ForSetting'/>
                </td>
                <td align="right">
                    应付金额:
                </td>
                <td>
                    <input type="text"  style="width:60px"  id="shouldPay_ForSetting" name="shouldPay_ForSetting"
                          value='@Model.shouldPay_ForSetting'/>
                </td>
                <td align="right">
                    客户名:
                </td>
                <td>
                    <input type="text"  style="width:60px"  id="wbCompany_ForSetting" name="wbCompany_ForSetting"
                          value='@Model.wbCompany_ForSetting'/>
                </td>
                <td align="right">
                    应付单位:
                </td>
                <td>
                    <input type="text" id="Receipt_ForSetting" name="Receipt_ForSetting" style="width: 80px" />
                    <input type="hidden" id="hid_Receipt_ForSetting" name="hid_Receipt_ForSetting"  value="@Model.Receipt_ForSetting"/>
                </td>
                <td align="right">
                    检验检疫拆包费:
                </td>
                <td>
                    <input type="text" id="QuarantinePacketFee_ForSetting" name="QuarantinePacketFee_ForSetting" style="width: 60px"   value='@Model.QuarantinePacketFee_ForSetting'/>
                </td>
                <td colspan="2">
                    <a href="#" id="btnUpdateFeeFore" name="btnUpdateFeeFore" class="easyui-linkbutton"
                        data-options="iconCls:'icon-edit'">修改</a>
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
