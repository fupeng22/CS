@{
    ViewBag.Title = "Index";
    Layout = "~/Views/CustomerMain/_Layout.cshtml";
}
<div id="layout_Main" class="easyui-layout" data-options="fit:true,border:false">
    <div region="north" split="false" style="height: 58px;" border="true">
        <table style="width: 100%; height: 100%; border-width: 0px" cellpadding="0px" cellspacing="0px">
            <tr style="width: 100%">
                <td style="background-image: url('../../images/Customer/top.jpg');">
                    <table>
                        <tr>
                            <td rowspan="2" style="width: 550px">
                            </td>
                            <td style="width: 600px">
                                <span class="bs"><a class="styleswitch black" style="cursor: pointer" rel="black"></a>
                                    <a class="styleswitch bootstrap" style="cursor: pointer" rel="bootstrap"></a><a class="styleswitch cupertino"
                                        style="cursor: pointer" rel="cupertino"></a>&nbsp;&nbsp;<a class="styleswitch dark-hive"
                                            style="cursor: pointer" rel="dark-hive"></a>&nbsp;&nbsp;<a class="styleswitch gray"
                                                style="cursor: pointer" rel="gray"></a>&nbsp;&nbsp;<a class="styleswitch default"
                                                    style="cursor: pointer" rel="default"></a> &nbsp;&nbsp;<a class="styleswitch metro"
                                                        style="cursor: pointer" rel="metro"></a> &nbsp;&nbsp;<a class="styleswitch metro-blue"
                                                            style="cursor: pointer" rel="metro-blue"></a> &nbsp;&nbsp;<a class="styleswitch metro-gray"
                                                                style="cursor: pointer" rel="metro-gray"></a> &nbsp;&nbsp;<a class="styleswitch metro-green"
                                                                    style="cursor: pointer" rel="metro-green"></a> &nbsp;&nbsp;<a class="styleswitch metro-orange"
                                                                        style="cursor: pointer" rel="metro-orange"></a>
                                    &nbsp;&nbsp;<a class="styleswitch metro-red" style="cursor: pointer" rel="metro-red"></a>
                                    &nbsp;&nbsp;<a class="styleswitch pepper-grinder" style="cursor: pointer" rel="pepper-grinder"></a>
                                    &nbsp;&nbsp;<a class="styleswitch sunny" style="cursor: pointer" rel="sunny"></a>
                                </span>
                            </td>
                            <td rowspan="2">
                                <a href="#" id="btnRelogin" class="easyui-linkbutton" data-options="iconCls:'icon-exit'">
                                    注销</a>&nbsp;&nbsp;&nbsp;
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span id="lblCurrentDate"></span>&nbsp;&nbsp;&nbsp; <span style="font-weight: bold;
                                    font-size: medium">当前登录:<font style="color: Red">@Session["Global_Customer_UserName"]</font>
                                </span>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
    </div>
    <div region="west" split="true" style="width: 150px;" border="false">
        <div id="LeftNavigation" class="easyui-accordion" fit="true">
            <div title="首页" style="overflow: auto;">
                <div class="cls_vertmenu">
                    <ul>
                        <li><a href="#" id="btnWelcome" name="btnWelcome" tabindex="1">首&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;页</a></li>
                    </ul>
                </div>
            </div>
            <div title="审单布控" style="overflow: auto;">
                <div class="cls_vertmenu">
                    <ul>
                        <li><a href="#" id="btnCheck" name="btnCheck" tabindex="1">审&nbsp;&nbsp;单&nbsp;&nbsp;布&nbsp;&nbsp;控</a></li>
                    </ul>
                </div>
            </div>
            <div title="审单放行" style="overflow: auto;">
                <div class="cls_vertmenu">
                    <ul>
                        <li><a href="#" id="btnConfirm" name="btnConfirm" tabindex="1">审&nbsp;&nbsp;单&nbsp;&nbsp;放&nbsp;&nbsp;行</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div region="center" style="background: #eee;" border="false" split="true">
        <div id="tab_Content" class="easyui-tabs" fit="true" border="false">
            <div title="使用指南" style="padding: 0px;">
                <center>
                    <table style="width: 503px; height: 225px;">
                        <tr style="height: 30px">
